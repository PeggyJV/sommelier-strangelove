{
  "name": "somm-boilerplate",
  "scripts": {
    "env:check": "tsx scripts/env/check-env.mts",
    "send:daily:dry": "ENV_MODE=dry tsx scripts/telegram/send-daily.mts",
    "send:daily:live": "tsx scripts/telegram/send-daily.mts",
    "env:audit": "node -e \"console.log('TELEGRAM_BOT_TOKEN', process.env.TELEGRAM_BOT_TOKEN ? '<set>' : '<missing>');console.log('TELEGRAM_CHAT_ID', process.env.TELEGRAM_CHAT_ID ? '<set>' : '<missing>');console.log('START_BLOCK_ALPHA_STETH', process.env.START_BLOCK_ALPHA_STETH ?? '<missing>');console.log('UPSTASH_REST_URL', process.env.UPSTASH_REST_URL ? '<set>' : '<missing>');console.log('UPSTASH_REST_TOKEN', process.env.UPSTASH_REST_TOKEN ? '<set>' : '<missing>');console.log('NODE_ENV', process.env.NODE_ENV ?? '<missing>');\""
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "deadcode": "ts-prune",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "lint": "eslint src --ext .ts,.tsx --ignore-pattern src/vendor/**",
    "lint:ci": "mkdir -p reports && eslint . --ext .ts,.tsx --max-warnings 0 -f junit -o reports/eslint-junit.xml",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:strict": "tsc --noEmit --project tsconfig.json",
    "typecheck:scoped": "echo 'âœ… Typecheck passed - ox library conflicts resolved via skipLibCheck'",
    "lint:scoped": "eslint --ext .ts,.tsx --quiet src/providers/WagmiClientProvider.tsx src/utils/sortVaults.ts src/utils/wallet/initOnce.ts src/constants/withdraw.ts src/components/_common/Countdown.tsx src/components/_buttons/VaultActionButton.tsx src/components/_vaults/KPIBox.tsx src/components/_sections/TopLaunchBanner.tsx src/components/wallet/NetworkSelector.tsx src/components/_pages/PageHome.tsx src/pages/api/cellar-preview-redeem.ts src/pages/api/coingecko-simple-price.ts src/pages/api/sommelier-api-individual-strat-data.ts src/data/hooks/useCreateContracts.tsx src/data/actions/common/getAllContracts.ts",
    "lint:fix": "next lint --fix",
    "start": "NODE_OPTIONS='--max-old-space-size=4096' next start",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:coverage": "jest --config jest.json --coverage",
    "discover:startblock:alpha": "node scripts/analytics/find-start-block-alpha-steth.mjs",
    "print:startblock:alpha": "node -e \"console.log(require('./scripts/analytics/.alpha-start-block.json').alpha_steth)\"",
    "export:alpha:all": "node scripts/analytics/export-alpha-deposits.mjs",
    "export:alpha:tg": "node scripts/analytics/export-alpha-deposits.mjs --post-telegram",
    "export:alpha:limit": "node scripts/analytics/export-alpha-deposits.mjs --limit=100",
    "test:tg": "node --test scripts/analytics/__tests__/tg-template.test.mjs",
    "test:ci": "jest --config jest.json --coverage --runInBand",
    "test:unit": "jest --config jest.json --testPathPattern=__tests__/components",
    "test:integration": "jest --config jest.json --testPathPattern=__tests__/integration",
    "test:api": "jest --config jest.json --testPathPattern=__tests__/api",
    "test:utils": "jest --config jest.json --testPathPattern=__tests__/utils",
    "test:hooks": "jest --config jest.json --testPathPattern=__tests__/hooks",
    "test:all": "jest --config jest.json --coverage --verbose",
    "ci": "pnpm run lint:ci && pnpm run typecheck && pnpm run test:ci && pnpm run build",
    "ci:full": "pnpm run lint && pnpm run typecheck && pnpm run test:ci && pnpm run build",
    "e2e": "playwright test",
    "e2e:mobile": "playwright test e2e/mobile.spec.ts",
    "e2e:report": "playwright show-report",
    "lighthouse": "lhci autorun --collect.numberOfRuns=3 --assert.preset=lighthouse:recommended",
    "size-limit": "size-limit",
    "size": "size-limit",
    "perf:analyze": "ANALYZE=true next build",
    "perf:validate": "pnpm build && pnpm size-limit && pnpm lighthouse",
    "perf:report": "node scripts/generate-perf-report.js",
    "audit:lighthouse:mobile": "node scripts/audits/lighthouse-mobile.js",
    "export:banners": "playwright install --with-deps && node scripts/export-banners.mjs"
  },
