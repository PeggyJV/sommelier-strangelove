{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "url": ["http://localhost:3000/"],
      "startServerCommand": "pnpm start",
      "startServerReadyPattern": "started server on",
      "settings": {
        "preset": "desktop"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci-reports"
    },
    "assert": {
      "preset": "lighthouse:recommended"
    }
  }
}
